    "method": "GET",
    "data_path": "",
    "field_mapping": {
      "Name": ["nameTranslated", "shortName", "name", "establishment_name"],
      "Address Line 1": ["streetAddress", "shortAddress", "address"],
      "City": ["cityName", "city"],
      "State/Province/Region": ["regionName", "state", "province"],
      "Country": ["countryName", "country"],
      "Postal/ZIP Code": ["postalCode", "zipCode", "zip"],
      "Phone": ["phone1", "phone"],
      "Email": "email",
      "Website": ["urlRolexV7", "website"],
      "Latitude": ["lat", "latitude"],
      "Longitude": ["lng", "longitude"]
    }
  },

