  "bell_ross_stores": {
    "type": "json",
    "url": "https://stores.bellross.com/search?q=48.8566%2C2.3522&r=2000&l=en&per=50&offset=0",
    "description": "Bell & Ross Store Locator - Radius-based search API with offset pagination (requires Accept: application/json header). Uses radius=2000km from Paris center point, returns 50 stores per page. Scraper automatically follows offset pagination to get all 242+ stores worldwide ✅",
    "method": "GET",
    "data_path": "response.entities",
    "field_mapping": {
      "Handle": "profile.meta.id",
      "Name": "profile.name",
      "Address Line 1": "profile.address.line1",
      "Address Line 2": "profile.address.line2",
      "City": "profile.address.city",
      "State/Province/Region": "profile.address.region",
      "Country": "profile.address.countryCode",
      "Postal/ZIP Code": "profile.address.postalCode",
      "Phone": "profile.mainPhone.display",
      "Latitude": "profile.geocodedCoordinate.lat",
      "Longitude": "profile.geocodedCoordinate.long"
    }
  },

  "custom_json_api": {
    "type": "json",
    "url": "https://api.yourbrand.com/stores",
