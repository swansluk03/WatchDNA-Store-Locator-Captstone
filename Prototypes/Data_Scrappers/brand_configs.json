  "breitling_stores": {
    "type": "json",
    "url": "https://store.breitling.com/en?country=US&qp=United+States&r=1000&l=en&per=50&offset=0",
    "description": "Breitling Store Locator - JSON API with country-based search and offset pagination. URL returns JSON directly. Use country parameter to get stores for different countries. Large radius (1000) gets all stores per country. Scraper handles offset pagination automatically ✅",
    "method": "GET",
    "data_path": "response.entities",
    "field_mapping": {
      "Handle": "profile.meta.id",
      "Name": "profile.name",
      "Address Line 1": "profile.address.line1",
      "Address Line 2": "profile.address.line2",
      "City": "profile.address.city",
      "State/Province/Region": "profile.address.region",
      "Country": "profile.address.countryCode",
      "Postal/ZIP Code": "profile.address.postalCode",
      "Phone": "profile.mainPhone.display",
      "Email": "profile.emails.0",
      "Website": "profile.websiteUrl.url",
      "Latitude": "profile.geocodedCoordinate.latitude",
      "Longitude": "profile.geocodedCoordinate.longitude"
    },
    "use_watch_store_countries": true,
    "_note": "When use_watch_store_countries is true, scraper uses comprehensive list from watch_store_countries.json. Set to false and provide 'countries' object to use custom list."
  }
